{
	"name": "adl-cutlang-ext",
	"description": "adl-cutlang",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:adlCutlang.colorsView",
		"onCommand:adlCutlang.addColor",
		"onCommand:adlCutlang.clearColors"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "adlCutlang.colorsView",
					"name": "Calico Colors"
				}
			]
		},
		"commands": [
			{
				"command": "adlCutlang.addColor",
				"category": "Calico Colors",
				"title": "Add Color"
			},
			{
				"command": "adlCutlang.clearColors",
				"category": "Calico Colors",
				"title": "Clear Colors",
				"icon": "$(clear-all)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "adlCutlang.clearColors",
					"group": "navigation",
					"when": "view == adlCutlang.colorsView"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"eslint": "^8.13.0",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"@types/vscode-webview": "^1.57.0"
	}
}
